{
  "name": "deasciifier",
  "version": "2.0.0",
  "description": "Turkish text deasciifier",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "preunit-test": "tsc --project test/",
    "unit-test": "mocha out/test/test.js && mocha out/test/view_test.js && karma start karma.conf.js",
    "preinteractive-test": "tsc --project interactive_test/ && browserify app.ts --standalone DeasciifierApp -p [ tsify --noImplicitAny ] -o out/test-bundle.js",
    "interactive-test": "node_modules/protractor/bin/protractor interactive_test/protractor.conf.js",
    "test": "npm run -s unit-test && npm run -s interactive-test",
    "watch": "watchify app.ts --standalone DeasciifierApp -p [ tsify --noImplicitAny ] -o ../../website/static/ts/bundle.js",
    "build": "browserify app.ts --standalone DeasciifierApp -p [ tsify --noImplicitAny ] -o ../../website/static/ts/bundle.js"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.33",
    "browserify": "^13.1.1",
    "chai": "^3.5.0",
    "codemirror": "^5.22.0",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-commonjs-require": "0.0.3",
    "karma-mocha": "^1.3.0",
    "karma-requirejs": "^1.1.0",
    "mocha": "^3.2.0",
    "protractor": "^4.0.14",
    "requirejs": "^2.3.2",
    "tsify": "^2.0.7",
    "typescript": "^2.1.4",
    "uglify": "^0.1.5",
    "watchify": "^3.8.0",
    "webdriver-manager": "^11.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meacer/deasciifier.git"
  },
  "bugs": {
    "url": "https://github.com/meacer/deasciifier/issues"
  },
  "homepage": "https://deasciifier.com",
  "author": "meacer",
  "license": "MIT",
  "dependencies": {
    "@types/codemirror": "0.0.35",
    "google-closure-compiler": "^20161201.0.0"
  }
}
